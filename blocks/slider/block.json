{
    "apiVersion": 2,
    "version": "1.0.0",
    "name": "acf/slider",
    "title": "Slider",
    "description": "A custom slider.",
    "category": "design",
    "icon": "slides",
    "keywords": [],
    "acf": {
        "mode": "preview",
        "renderTemplate": "slider.php"
    },
    "supports": {
        "anchor": true,
        "align": [ "full" ],
        "ariaLabel": true,
        "background": {
            "backgroundImage": true,
            "backgroundSize": true
        },
        "className": true,
        "color": {
            "background": true,
            "button": true,
            "heading": true,
            "link": true,
            "text": true
        },
        "shadow": true,
        "spacing": {
            "margin": true,
            "padding": true,
            "blockGap": true
        },
        "typography": {
            "fontSize": true,
            "lineHeight": true,
            "textAlign": true
        }
    },
    "attributes": {
        "style": { "type": "object" },
        "align": { "type": "string","default": "full" }
    }
}